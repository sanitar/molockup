<%= form_for [@project, @mockup] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :project_id %><br />
    <%= f.number_field :project_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
